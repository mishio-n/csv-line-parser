{
  "compilerOptions": {
      "target": "ESNext",
      "module": "commonjs",
      "declaration": true,
      "removeComments": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "esModuleInterop": true,
      "newLine": "lf"
  },
  "exclude": ["test"]
}